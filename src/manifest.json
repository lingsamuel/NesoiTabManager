{
    "name": "Nesoi Tab Manager",
    "version": "0.1.0",
    "description": "A simple but powerful tab manager which aims to organize thousands of tabs and bookmarks.",
    "background": {
        "scripts": [
            "background.js"
        ],
        "persistent": false
    },
    "browser_action": {
    },
    "permissions": [
        "contextMenus",
        "storage",
        "tabs",
        "unlimitedStorage"
    ],
    "options_page": "index.html",
    "options_ui": {
        "page": "index.html",
        "open_in_tab": true
    },
    "content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self'",
    "manifest_version": 2
}
